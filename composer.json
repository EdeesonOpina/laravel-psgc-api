{
    "name": "edeesonopina/laravel-psgc-api",
    "description": "Philippine Standard Geographic Code (PSGC) API package for Laravel - Official PSA data with import/export functionality",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "psgc",
        "philippines",
        "geographic",
        "api",
        "laravel",
        "psa",
        "regions",
        "provinces",
        "cities",
        "municipalities",
        "barangays"
    ],
    "authors": [
        {
            "name": "Edeeson Opina",
            "email": "contact@edeesonopina.vercel.app",
            "homepage": "https://edeesonopina.vercel.app"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/console": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "EdeesonOpina\\PsgcApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EdeesonOpina\\PsgcApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EdeesonOpina\\PsgcApi\\PsgcApiServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"file_exists('database/migrations') || mkdir('database/migrations', 0755, true);\"",
            "@php artisan vendor:publish --provider=\"EdeesonOpina\\PsgcApi\\PsgcApiServiceProvider\" --tag=\"psgc-migrations\"",
            "@php artisan vendor:publish --provider=\"EdeesonOpina\\PsgcApi\\PsgcApiServiceProvider\" --tag=\"psgc-config\""
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --provider=\"EdeesonOpina\\PsgcApi\\PsgcApiServiceProvider\" --tag=\"psgc-migrations\" --force",
            "@php artisan vendor:publish --provider=\"EdeesonOpina\\PsgcApi\\PsgcApiServiceProvider\" --tag=\"psgc-config\" --force"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}